{
  "name": "@epklabs/pushpop",
  "version": "2.0.0",
  "description": "a front end client for PushPop written in typescript ",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "pushpop",
    "client",
    "websocket",
    "typescript"
  ],
  "engines": {
    "node": ">=20.18.2"
  },
  "homepage": "https://github.com/biohackerellie/pushpop",
  "repository": {
    "type": "git",
    "url": "https://github.com/biohackerellie/pushpop.git"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "dev": "tsup --watch",
    "lint": "npx @biomejs/biome lint",
    "clean": "rm -rf dist node_modules",
    "release": "changeset version",
    "pub:release": "pnpm build && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "author": {
    "name": "BiohackerEllie",
    "url": "https://github.com/biohackerellie",
    "email": "ellie@epklabs.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.12.0",
    "np": "^10.2.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.6.1"
  }
}
