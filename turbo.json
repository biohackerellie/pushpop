{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".turbo/.cache/tsbuildinfo.json"
      ]
    },
    "//#build:go": {
      "dependsOn": ["^build"],
      "outputs": [
      "bin/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "//#dev:root": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^topo"],
      "outputs": [".turbo/.cache/tsbuildinfo.json"]
    },
    "//#lint:root": {
      "dependsOn": ["^topo"]
    },
    "typecheck": {
      "dependsOn": ["^topo"],
      "outputs": [".turbo/.cache/tsbuildinfo.json"]
    }
  }
}
