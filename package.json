{
  "name": "pushpop/root",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "release": "changeset version",
    "pub:release": "cd ts-client && pnpm pub:release",
    "lint": "turbo run lint lint:root",
    "lint:root": "golangci-lint run ./...",
    "clean": "git clean -xdf node_modules",
    "build": "turbo run build build:root",
    "build:root": "make build-go",
    "dev": "turbo run dev dev:root --parallel",
    "dev:root": "air -c .air.toml",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.27.3"
  },
  "packageManager": "pnpm@9.14.4",
  "devDependencies": {
    "turbo": "^2.3.3"
  }
}
